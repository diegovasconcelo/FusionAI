version: '3.2'

services:
    db:
        image: postgres
        environment: 
            - POSTGRES_DB=
            - POSTGRES_USER=
            - POSTGRES_PASSWORD=

    web_fai: 
        build: .
        command: python manage.py runserver 0.0.0.0:8000
        volumes:
            - .:/app
        ports:
            - "8000:8000"
        depends_on: 
            - db